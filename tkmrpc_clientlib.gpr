with "tkmrpc_common";

library project TKMRPC_Clientlib is

   for Languages use ("Ada", "C");
   for Source_Dirs use TKMRPC_Common.Source_Dirs & "interfaces/c";
   for Object_Dir use "obj/clientlib";
   for Library_Dir use "lib";
   for Library_Kind use "static";
   for Library_Name use "tkmrpc";

   Compiler_Switches := TKMRPC_Common.Compiler_Switches & "-gnatwale";

   package Compiler is
      for Default_Switches ("ada") use Compiler_Switches;
   end Compiler;

end TKMRPC_Clientlib;

